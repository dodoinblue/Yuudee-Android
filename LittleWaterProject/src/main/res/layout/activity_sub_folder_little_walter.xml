<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@android:color/transparent"
    android:gravity="left|center_vertical"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/back_to_main_ui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="70dp"
        android:layout_marginBottom="@dimen/return_button_margin"
        android:clickable="true"
        android:onClick="onClick"
        android:src="@mipmap/child_incat_backbutton" />

    <com.gcwt.yudee.widget.ScrollLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@mipmap/child_incat_woodbg"
        android:gravity="center" />
</LinearLayout>
